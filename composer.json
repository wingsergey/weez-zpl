{
    "name": "wingsergey/weez-zpl",
    "description": "Project weez-zpl, forked from teddy-dubal/weez-zpl. This one is focused on its maintenance.",
    "authors": [
        {
            "name": "Stephan de Souza",
            "email": "stephan@gblix.net"
        }
    ],
    "require": {
        "php": "^7.3 || ^8",
        "wingsergey/zebra": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpstan/phpstan": "^0.12.55",
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5.5"
    },
    "suggest": {
        "ext-gd": "For image processing"
    },
    "autoload": {
        "psr-0": {
            "Weez\\Zpl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Weez\\Zpl\\Tests\\": "tests"
        }
    }
}
